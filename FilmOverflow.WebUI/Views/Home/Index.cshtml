@{
	ViewBag.Title = "FilmOverflow";
}

<div class="container">
	<figure class="well well-lg text-center font-bordered ">
		<figcaption>
			<h1>@ViewBag.Title</h1>
		</figcaption>
		<img class="img-main" src="@Url.Content("~/Content/Images/Background-images/main.jpg")" />
		@Html.ActionLink(" Schedule", "Browse", "Home", null, new { @class = "btn  btn-primary fa fa-lg fa-film" })
	</figure>
</div>

<div class="container">
	<div class="row">
		@{ Html.RenderAction("FilmList", "Home"); }
	</div>
</div>
